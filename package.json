{
  "name": "go-tags",
  "displayName": "Go Tags",
  "description": "Add tags for struct in Go",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "author": {
    "name": "GY",
    "email": "dev.youngkwok718@gmail.com"
  },
  "publisher": "whosydd",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "url": "https://github.com/whosydd/go-tags.git"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [],
    "configuration": [
      {
        "title": "associations",
        "properties": {
          "go-tags.associations": {
            "type": "array",
            "scope": "language-overridable",
            "default": [
              "json",
              "xml",
              "yaml"
            ]
          }
        }
      }
    ]
  },
  "scripts": {
    "vsce:package": "pnpm vsce package --no-dependencies",
    "vsce:publish": "pnpm vsce publish --no-dependencies",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
